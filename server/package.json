        "mongoose": "^7.2.2",
        "nodemailer": "^7.0.3"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.18",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^20.4.2",
